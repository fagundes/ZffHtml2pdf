{
    "name": "fagundes/zff-html2pdf",
    "type": "library",
    "description": "A ZF2 module using html2pdf library",
    "keywords": ["html", "pdf", "html2pdf", "zf2"],
    "homepage": "http://github.com/fagundes/ZffHtml2pdf",
    "license": "MIT",
    "authors":
    [
        {
            "name": "Vinicius Fagundes",
            "homepage": "https://github.com/fagundes",
            "role": "Developer"
        }
    ],
    "require":
    {
        "php": ">=5.4",
        "zendframework/zend-view":"~2.4",
        "zendframework/zend-http":"~2.4",
        "spipu/html2pdf":"~4.4"
    },
    "require-dev": {
        "zendframework/zend-servicemanager":"~2.4",
        "zendframework/zend-modulemanager":"~2.4",
        "zendframework/zend-mvc":"~2.4",
        "zendframework/zend-filter":"~2.4",
        "zendframework/zend-serializer":"~2.4",
        "zendframework/zend-log":"~2.4",
        "zendframework/zend-i18n":"~2.4",
        "zendframework/zend-console":"~2.4",
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2",
        "thomasbachem/php-short-array-syntax-converter": "dev-master"
    },
    "autoload":
    {
        "psr-4":
        {
            "Zff\\Html2Pdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZffTest\\Html2Pdf\\": "test/"
        }
    },
    "extra":
    {
        "branch-alias": {
            "dev-master": "0.2.x-dev",
            "dev-develop": "0.3.x-dev"
        }
    }
}
